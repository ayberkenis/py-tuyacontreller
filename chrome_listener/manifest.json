{
  "name": "WNWD",
  "description": "Whatsapp Notification Webhook Dispatcher for bulb-notifications-python",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions" : ["*://*.whatsapp.com/"],
  "permissions": ["storage", "tabs", "scripting", "webRequest"]
}
